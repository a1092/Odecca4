<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Odecca4</display-name>
  <servlet>
    <description></description>
    <display-name>LoginController</display-name>
    <servlet-name>LoginController</servlet-name>
    <servlet-class>fr.efrei.Security.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginController</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>HomeController</display-name>
    <servlet-name>HomeController</servlet-name>
    <servlet-class>fr.efrei.Home.HomeController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HomeController</servlet-name>
    <url-pattern>/app/home</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>SessionFilter</filter-name>
    <filter-class>fr.efrei.Security.SessionFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>SessionFilter</filter-name>
    <url-pattern>/app/*</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>LogoutController</display-name>
    <servlet-name>LogoutController</servlet-name>
    <servlet-class>fr.efrei.Security.LogoutController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutController</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AccountController</display-name>
    <servlet-name>AccountController</servlet-name>
    <servlet-class>fr.efrei.Account.AccountController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AccountController</servlet-name>
    <url-pattern>/app/account</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>login</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>InterimaireController</display-name>
    <servlet-name>InterimaireController</servlet-name>
    <servlet-class>fr.efrei.Interimaire.InterimaireController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InterimaireController</servlet-name>
    <url-pattern>/app/interimaire</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CompetenceController</display-name>
    <servlet-name>CompetenceController</servlet-name>
    <servlet-class>fr.efrei.Interimaire.CompetenceController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CompetenceController</servlet-name>
    <url-pattern>/app/competence</url-pattern>
  </servlet-mapping>
</web-app>